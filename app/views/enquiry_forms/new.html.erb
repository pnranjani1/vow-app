<%= form_for(@enquiry_form) do |f| %>

<label> Name </label><br/>
<%= f.text_field :name %><br><p>

<label> Email </label><br/>
<%= f.email_field :email %> <br/><p>

<label> Phone Number</label><br/>
<%= f.text_field :phone_number %> <br/><p>


<label> Comments </label><br/>
<%= f.text_area :comments %> <br/><p>

<%= f.submit "Submit" %>
  <% end %>
